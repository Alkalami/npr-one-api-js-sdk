{
  "name": "@npr/npr-one-sdk",
  "version": "0.0.0-semantically-released",
  "homepage": "http://npr.github.io/npr-one-api-js-sdk/",
  "description": "An SDK for interfacing with the NPR One API, designed as a Universal Javascript Module",
  "keywords": [
    "npr",
    "nprone",
    "radio",
    "sdk"
  ],
  "author": "NPR Enterprise Team <NPROneEnterprise@npr.org> (http://dev.npr.org/)",
  "contributors": [
    "Jared Biehler <jaredbcoding@gmail.com>",
    "Nara Kasbergen <nara.kasbergen@gmail.com>"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/npr/npr-one-api-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/npr/npr-one-api-js-sdk/issues",
    "email": "NPROneEnterprise@npr.org"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/node/index.js",
  "browser": "./dist/browser/npr-one-sdk.js",
  "typings": "./dist/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "test",
    "example": "examples"
  },
  "scripts": {
    "update": "npm prune && npm cache verify && npm install",
    "lint": "tslint -p tsconfig.json -t stylish",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix",
    "lint:report": "tslint -p tsconfig.json -t checkstyle -o eslint.xml || true",
    "compile": "npm run compile:node && npm run compile:browser",
    "compile:node": "rimraf ./dist/node && tsc",
    "compile:browser": "webpack",
    "pretest": "rimraf ./test-reports && mkdir test-reports/",
    "test": "jest --config .jestconfig.json",
    "test:ci": "JEST_JUNIT_OUTPUT=./test-reports/results.xml npm run test -- --ci --testResultsProcessor=\"jest-junit\"",
    "test:watch": "npm run test -- --watch",
    "test:debug": "npm run test -- --verbose",
    "test:coverage:upload": "cat ./test-reports/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "predocs": "rimraf ./docs",
    "docs": "esdoc -c esdoc.json",
    "commit": "git-cz",
    "prepare": "not-in-install && npm run compile || true",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "es6-error": "4.1.1",
    "isomorphic-fetch": "2.2.1",
    "js-logger": "1.4.1",
    "url-parse": "1.4.0"
  },
  "devDependencies": {
    "@types/fetch-mock": "6.0.3",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jest": "22.2.3",
    "@types/node": "~8.10.10",
    "ajv": "6.5.0",
    "clean-webpack-plugin": "0.1.19",
    "commitizen": "2.10.1",
    "coveralls": "3.0.1",
    "cz-conventional-changelog": "2.1.0",
    "fetch-mock": "6.4.3",
    "in-publish": "2.0.0",
    "jest": "23.1.0",
    "jest-junit": "5.0.0",
    "marked": "0.4.0",
    "mockery": "2.1.0",
    "rimraf": "2.6.2",
    "semantic-release": "15.5.0",
    "shared-git-hooks": "1.2.1",
    "ts-loader": "4.3.0",
    "tslint": "5.10.0",
    "tslint-config-airbnb": "5.9.2",
    "typedoc": "0.11.1",
    "typescript": "2.9.1",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.10.2"
  },
  "engines": {
    "node": ">= 6.9",
    "npm": ">= 3.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
